ntries\n        .map((entry) => entry?.selectedRegion?.id)\n        .filter((id) => id !== null && id !== undefined);\n    },\n  },\n  methods: {\n    handleRegionClick(regionId) {\n      this.$emit(\"region-clicked\", regionId);\n    },\n  },\n  mounted() {\n    if (this.simplemaps_type?.load) {\n      this.filteredRegions.load();\n    }\n  },\n};\n</script>\n"]}]}